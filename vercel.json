{
    "version": 2,
   "builds": [
    {
      "src": "app/routes/adAccount_routes.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.12" }  
    },
    {
      "src": "app/routes/adimages_routes.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.12" } 
    },
    {
      "src": "app/routes/adset_routes.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.12" } 
    },
    {
      "src": "app/routes/ad_routes.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.12" } 
    },
    {
      "src": "app/routes/ad_videos.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.12" } 
    },
    {
      "src": "app/routes/campaign_routes.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.12" } 
    },
    {
      "src": "app/routes/creative_routes.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.12" } 
    }
  ],
    "routes": [
      {
        "src": "/ad",
        "dest": "app/routes/ad_routes.py"
      },
      {
        "src": "/ad/<string:id>",
        "dest": "app/routes/ad_routes.py"
      },
      {
        "src": "/ads",
        "dest": "app/routes/ad_routes.py"
      },
      {
        "src": "/advideo",
        "dest": "app/routes/ad_videos.py"
      },
      {
        "src": "/advideos",
        "dest": "app/routes/ad_videos.py"
      },
      {
        "src": "/adset",
        "dest": "app/routes/adset_routes.py"
      },
      {
        "src": "/adset/<string:id>",
        "dest": "app/routes/adset_routes.py"
      },
      {
        "src": "/adsets",
        "dest": "app/routes/adset_routes.py"
      },
      {
        "src": "/campaign",
        "dest": "app/routes/campaign_routes.py"
      },
      {
        "src": "/campaign/<string:id>",
        "dest": "app/routes/campaign_routes.py"
      },
      {
        "src": "/campaigns",
        "dest": "app/routes/campaign_routes.py"
      },
      {
        "src": "/creative",
        "dest": "app/routes/creative_routes.py"
      },
      {
        "src": "/creative/<string:id>",
        "dest": "app/routes/creative_routes.py"
      },
      {
        "src": "/creatives",
        "dest": "app/routes/creative_routes.py"
      }
    ]
  }
  